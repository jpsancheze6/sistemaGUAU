      <TextField fx:id="txtNombre" layoutX="189.0" layoutY="94.0" promptText="Nombre" />
      <TextField fx:id="txtTelefono" layoutX="189.0" layoutY="136.0" promptText="Telefono" />
      <TextField fx:id="txtEmpresa" layoutX="189.0" layoutY="172.0" promptText="Empresa" />
      <ComboBox fx:id="cmbProducto" layoutX="188.0" layoutY="231.0" prefWidth="150.0" />
      <Label layoutX="189.0" layoutY="206.0" text="Producto">
